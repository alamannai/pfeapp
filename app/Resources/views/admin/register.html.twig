{% extends 'admin/admin.html.twig' %}



{% block body %}
  <div class="col-lg-12">
    <div class="card">
      <div class="card-header">Registrer une commune</div>
      <div class="card-body">
        <form>
          <div class="form-group">
        
              <div class="col-md-6">
                <label for="nom">Nom</label>
                <input class="form-control" id="nom" type="text" aria-describedby="nameHelp" placeholder="Entrer Nom">
              </div>
              <div class="col-md-6">
                <label for="gouvernorat">Gouvernorat</label>
                <select class="form-control">
                  <option selected="" >Choisir gouvernorat</option>
                  <option>Nabeul</option>
                  <option>Tunis</option>
                  <option>Sfax</option>
                  <option>Zaghouen</option>
                </select>
              </div>
            
          </div>
          <div class="form-group">
            <div class="col-md-12">
            <label for="pseudo">Pseudo</label>
            <input class="form-control" id="pseudo" type="email" aria-describedby="emailHelp" placeholder="Entrer Pseudo">
          </div>
          </div>
          <div class="form-group">
            <div class="form-row">
              <div class="col-md-6">
                <label for="exampleInputPassword1">Password</label>
                <input class="form-control" id="exampleInputPassword1" type="password" placeholder="Password">
              </div>
              <div class="col-md-6">
                <label for="exampleConfirmPassword">Confirm password</label>
                <input class="form-control" id="exampleConfirmPassword" type="password" placeholder="Confirm password">
              </div>
            </div>
          </div>
          <div class="col-md-3">
          <a class="btn btn-primary btn-fill  " href="#">Register</a>
        </div>
        </form>
       
      </div>
    </div>
  </div>
{% endblock %}